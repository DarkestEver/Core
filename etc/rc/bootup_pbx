require_once("globals.php");
while (true){
    $pid = Util::get_pid_process('beanstalkd');
    if(empty($pid)){
        sleep(2);
    }else{
        $g['booting'] = TRUE;
        Mikopbx\Main::bootup_pbx();
        $g['booting'] = FALSE;
        break;
    }
}

